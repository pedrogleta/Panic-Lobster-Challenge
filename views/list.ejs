<%- include ('partials/header') %> 

<div class="container">

<div class="container d-flex justify-content-center m-5 jumbotron">
<h1 class="display-4">List of Registrations</h1>
</div>

<ul class="nav nav-pills m-3">
    <li class="nav-item">
        <a class="nav-link" id="plc" href="/">New Registration</a>
    </li>
    <li class="nav-item">
        <a class="nav-link active" id="plbg" href="#">List of Registrations</a>
    </li>
</ul>
</div>

<div class="container">
    <table class="table table-dark">
        <thead>
        <tr>
            <th scope="col">Full Name</th>
            <th scope="col">Email address</th>
            <th scope="col">Birthdate</th>
            <th scope="col">Individual Registry (CPF)</th>
        </tr>
        </thead>
        <tbody>
        
        <% users.forEach(function(user){ %>
            <tr>
                <td> <%= user.name %> </td>
                <td> <%= user.email %> </td>
                <td> <%= user.birthdate %> </td>
                <td> <%= user.cpf %> </td>
            </tr>
        <% }) %> 
        </tbody>
    </table>
</div>

<%- include('partials/footer') %>